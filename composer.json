{
  "name": "systemhaus/guzzle-http-mock",
  "description": "A mock library for verifying requests made with the Guzzle Http Client, and mocking responses.",
  "keywords": [
    "testing",
    "mocking",
    "mock",
    "guzzle",
    "http",
    "client"
  ],
  "license": "GPL-3.0-or-later",
  "authors": [
    {
      "name": "Edan Schwartz",
      "email": "edanschwartz@gmail.com",
      "role": "Maintainer"
    },
    {
      "name": "Seth Miller",
      "email": "seth@four43.com",
      "role": "Maintainer"
    },
    {
       "name": "Nick Shipes",
       "email": "nshipes@aerisweather.com",
       "role": "Maintainer"
    }
  ],
  "require": {
    "php": ">=7.1||^8.0",
    "guzzlehttp/guzzle": "^7.0"
  },
  "conflict": {
    "aeris/guzzle-http-mock": "*"
  },
  "autoload": {
    "psr-4": {
      "Aeris\\GuzzleHttpMock\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Aeris\\GuzzleHttpMock\\Test\\": "test/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  }
}
